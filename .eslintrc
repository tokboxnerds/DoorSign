{
    "ecmaFeatures": {
        "arrowFunctions": true,
        "jsx": true
    },
    "rules": {
        "global-strict": false,
        "quotes": false,
        "comma-dangle": false,
        "no-trailing-spaces": [2, { "skipBlankLines": true }]
    },
    "plugins": [
      "react"
    ],

    "env": {
        "browser": true,
        "node": true,
        "es6": true
    }
}
